<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_type_installation" model="ir.sequence.type">
            <field name="name">Installation Form</field>
            <field name="code">installation</field>
        </record>

        <record id="seq_installation" model="ir.sequence">
            <field name="name">Installation No</field>
            <field name="code">installation</field>
            <field name="prefix"></field>
            <field name="suffix"></field>
            <field name="padding">3</field>
        </record>
        
        <record id="seq_type_warranty" model="ir.sequence.type">
            <field name="name">Warrant Form</field>
            <field name="code">warranty</field>
        </record>
        
         <record id="seq_warranty" model="ir.sequence">
            <field name="name">Warranty No</field>
            <field name="code">warranty</field>
            <field name="prefix">PS</field>
            <field name="suffix">$(day)s%(month)s%(year)s</field>
            <field name="padding">3</field>
        </record>
       

    </data>
</openerp>